<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:System;assembly=mscorlib">
    <Style x:Key="TextBox" TargetType="TextBox">
        <Setter Property="BorderThickness" Value="0,0,0,0"/>
        <Setter Property="Opacity" Value="0.5"/>
    </Style>
    <Style x:Key="Button" TargetType="Button">
        <Setter Property="BorderThickness" Value="0,0,0,0"/>
        <Setter Property="Background" Value="#F8C5B4"/>
    </Style>
    <Style TargetType="{x:Type Window}">
        <Setter Property="FontFamily" Value="Jura" />
    </Style>
    <!--<v:String x:Key="Main">#F8C5B4</v:String>
    <v:String x:Key="Second">#F8C5B4</v:String>
    <v:String x:Key="Focus">#F8C5B4</v:String>
    <v:String x:Key="Back">#FFFFFF</v:String>-->
</ResourceDictionary>