<UserControl x:Class="XXXTentacion_B_O6JlaKax.View.for_wrappanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:XXXTentacion_B_O6JlaKax.View"
             mc:Ignorable="d" Height="60" Width="150">
    <Grid Style="{DynamicResource GridSettingCart}" Width="150" Height="60">
        <Button Style="{DynamicResource panel}"  VerticalContentAlignment="Top" VerticalAlignment="Top" HorizontalAlignment="Right" Content="x" Background="{x:Null}" FontFamily="Jura" RenderTransformOrigin="0.5,0.5" Height="14" Width="10" Foreground="#FFFF"/>
        <TextBlock HorizontalAlignment="Left" Text="{Binding CityName, RelativeSource={RelativeSource AncestorType=UserControl}}" Margin="3,10,0,0" TextWrapping="Wrap" Foreground="#FFF" FontFamily="Jura" VerticalAlignment="Top" Width="137" TextAlignment="Center" Height="17" FontSize="14"/>
        <TextBlock HorizontalAlignment="Center" Text="{Binding CityCord, RelativeSource={RelativeSource AncestorType=UserControl}}" Style="{DynamicResource TextBlock}" Margin="0,32,0,0" VerticalAlignment="Top" Width="150" Height="29" FontSize="10"/>
    </Grid>
</UserControl>
